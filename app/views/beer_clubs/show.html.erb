<p id="notice"><%= notice %></p>

<h1> <%= @beer_club.name %> </h1>

<p>
  <strong>Founded:</strong>
  <%= @beer_club.founded %>
</p>

<p>
  <strong>City:</strong>
  <%= @beer_club.city %>
</p>

<h2>Members</h2>

<ul>
  <% @beer_club.users.each do |user| %>
    <li>
      <%= link_to user.username, user %>  
    </li>
  <% end %>
</ul> 

<%= link_to 'Edit', edit_beer_club_path(@beer_club) %> |
<%= link_to 'Back', beer_clubs_path %>
